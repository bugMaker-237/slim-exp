{
  "name": "slim-linq",
  "version": "0.0.1",
  "description": "Simple ts linq expression parser",
  "main": "index.ts",
  "scripts": {
    "start:dev": "nodemon",
    "build": "rimraf ./build && tsc",
    "format": "prettier --write \"src/**/*.(ts|js)\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bugMaker-237/slim-linq.git"
  },
  "keywords": [
    "linq",
    "expression",
    "parser"
  ],
  "author": "bugmaker-237",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bugMaker-237/slim-linq/issues"
  },
  "homepage": "https://github.com/bugMaker-237/slim-linq#readme",
  "devDependencies": {
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "jest": "^26.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
